 //1ms

class Solution {
    public int singleNonDuplicate(int[] nums) {
        if(nums.length <2){
            return nums[0];
        }
        else{
        for(int i=0;i<nums.length-1;i=i+2){
            if(nums[i]!=nums[i+1]){
                return nums[i];
            }
        }
        }
        return nums[nums.length-1];
    }
}

class Solution {
    public int singleNonDuplicate(int[] nums) {
        int left=0,right=nums.length-1;

        while(left<right){
            int mid=left+ (right-left) /2;

            if(mid % 2==1){
                mid--;
            }

            if(nums[mid] == nums[mid+1]){
                left=mid+2;
            } else {
                right=mid;
            }
        }
        return nums[left];
    }
}
